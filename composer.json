{
    "name": "super-admin-org/config",
    "description": "Config extension for super-admin, thanks to z-song",
    "type": "library",
    "keywords": ["super-admin", "setting"],
    "homepage": "https://github.com/super-admin-org/config",
    "license": "MIT",
    "authors": [
        {
            "name": "super-admin",
            "email": "infog@super-admin.org"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": ">=7.0",
        "super-admin-org/super-admin": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "laravel/laravel": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "SuperAdmin\\Admin\\Config\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SuperAdmin\\Admin\\Config\\ConfigServiceProvider"
            ]
        }
    }
}
